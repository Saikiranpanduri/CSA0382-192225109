#include<stdio.h>
int main(){
	int num[10],i;
	printf("Enter the elements:\n");
	for(i=0;i<=10;i++)
	scanf("%d",& num[i]);
	printf("Number of Even:\n");
	for(i=0;i<=10;i++){
		if(num[i]% 2 ==0){
			printf("%d\n",num[i]);
		}
	}
	printf("Number of Odd:\n");
	for(i=0;i<=10;i++){
		if(num[i]% 2 !=0){
			printf("%d\n",num[i]);
		}
	}
}
